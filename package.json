{
  "name": "REPLACEME",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "license": "MIT",
  "author": "Andrew Davis",
  "scripts": {
    "build": "rimraf dist/* && tsc",
    "dev:start": "NODE_ENV=development LOG_LEVEL=debug node --conditions=development --experimental-loader @savid/esbuild-register-loader -r esbuild-register -r dotenv/config src/index.ts",
    "dev": "nodemon --signal SIGHUP --watch src -e ts --exec npm run dev:start",
    "lint": "eslint .",
    "test": "jest --verbose"
  },
  "dependencies": {
    "@savid/logger": "1.2.0",
    "@savid/shutdown": "1.0.4",
    "dotenv": "16.0.2",
    "prom-client": "14.1.0"
  },
  "devDependencies": {
    "@savid/esbuild-register-loader": "1.0.1",
    "@savid/eslint": "1.2.0",
    "@types/jest": "29.0.0",
    "@types/node": "18.7.16",
    "esbuild": "0.15.7",
    "esbuild-register": "3.3.3",
    "jest": "29.0.3",
    "jest-config": "29.0.3",
    "jest-mock": "29.0.3",
    "nodemon": "2.0.19",
    "ts-jest": "29.0.0",
    "typescript": "4.8.3"
  },
  "volta": {
    "node": "18.7.0",
    "npm": "8.18.0"
  },
  "imports": {
    "#app/*": {
      "development": "./src/*.ts",
      "node": "./dist/*.js"
    }
  }
}
